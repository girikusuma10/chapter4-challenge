{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": "google",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "ignorePatterns": [
        "migrations",
        "**.min.js"
    ],
    "rules": {
        "linebreak-style":"off",
        "require-jsdoc": "off",
        "new-cap": "off",
        "no-tabs":"off",
        "indent":"off"
    }
}
